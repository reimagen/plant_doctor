# Server-only API key for Gemini (used by Cloud Run proxy + Cloud Functions)
GEMINI_API_KEY=

# Cloud Run WebSocket proxy URL (replaces NEXT_PUBLIC_GEMINI_API_KEY)
# e.g. wss://plant-doctor-ws-xxxx-uc.a.run.app
NEXT_PUBLIC_CLOUD_RUN_URL=

# Optional: Direct client-side Gemini key (fallback if no proxy URL set)
# NEXT_PUBLIC_GEMINI_API_KEY=

# Firebase config
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=
